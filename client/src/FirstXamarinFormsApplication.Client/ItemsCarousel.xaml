<?xml version="1.0" encoding="UTF-8" ?>
<CarouselPage
    x:Class="FirstXamarinFormsApplication.Client.ItemsCarousel"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:client="clr-namespace:FirstXamarinFormsApplication.Client;assembly=NetCore.Mobile.Client"
    xmlns:local="using:FirstXamarinFormsApplication.Client">
    <CarouselPage.Children>
        <client:ListItemView BindingContext="{Binding .}" />
        <client:ItemView BindingContext="{Binding Items[0]}" />
    </CarouselPage.Children>
</CarouselPage>
